<h1>Locações</h1>

<%= link_to 'Registrar nova locação', new_rental_path %>

<% @rentals.each do  |rental| %>
  <dl>
    <dt>Data de Inicío:</dt>
    <dd><%= l rental.start_date %></dt>
    <dt>Data de Término:</dt>
    <dd><%= l rental.end_date %></dt>
    <dt>Cliente:</dt>
    <dd><%= rental.customer.name%></dt>
    <dt>Categoria:</dt>
    <dd><%= rental.car_category.name %></dt>
  </dl>
<% end %>